    "sass-loader": "^4.0.1",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "stylelint": "^7.2.0",
    "stylelint-config-idiomatic-order": "^2.0.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1",
    "whatwg-fetch": "^1.0.0"
  }
